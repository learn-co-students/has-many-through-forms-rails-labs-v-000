<%= form_for comment do |f| %>
<%binding.pry%>
  <%= f.label "content" %>
  <%= f.text_field :comment_content %>
  <%= f.collection_check_boxes :user_ids, User.all, :id, :username %>
  <%= f.fields_for :users, comment.user.build do |user_fields| %>
    <%= user_fields.text_field :username %>
  <% end %>
  <%= f.submit %>
<% end %>
